{
    "apiVersion": "v1",
    "kind": "Template",
    "metadata": {
        "name": "hello"
    },
    "objects": [
        {
            "apiVersion": "v1",
            "kind": "Service",
            "metadata": {
                "name": "hello-service"
            },
            "containerPort": 8080,
            "spec": {
                "ports": [
                    {
                        "name": "hello",
                        "port": 5555
                    }
                ],
                "selector": {
                    "name": "hello-pod"
                }
            }
        },
        {
            "apiVersion": "v1",
            "kind": "Route",
            "metadata": {
                "name": "hello-route"
            },
            "spec": {
                "path": "/testpath1",
                "to": {
                    "kind": "Service",
                    "name": "hello-service"
                }
            }
        },
        {
            "apiVersion": "v1",
            "kind": "Pod",
            "metadata": {
                "name": "hello-pod"
            },
            "spec": {
                "containers": [
                    {
                        "image": "openshift/hello-openshift",
                        "name": "hello-pod",
                        "ports": [
                            {
                                "containerPort": 8080
                            }
                        ]
                    }
                ]
            }
        }
    ]
}
