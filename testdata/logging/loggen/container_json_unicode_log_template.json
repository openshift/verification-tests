{
    "apiVersion": "template.openshift.io/v1",
    "kind": "Template",
    "metadata": {
        "name": "centos-logtest-template"
    },
    "objects": [
        {
            "apiVersion": "v1",
            "data": {
                "ocp_logtest.cfg": "--raw --num-lines 50 --line-length 0 --file /var/lib/svt/unicode.example  --text-type input --rate 60.0",
                "unicode.example": "ㄅㄉˇˋㄓˊ˙ㄚㄞㄢㄦㄆ 中国 883.317µs ā á ǎ à ō ó ▅ ▆ ▇ █ 々"
            },
            "kind": "ConfigMap",
            "metadata": {
                "name": "logtest-config"
            }
        },
        {
            "apiVersion": "v1",
            "kind": "ReplicationController",
            "metadata": {
                "name": "centos-logtest",
                "labels": {
                    "run": "centos-logtest",
                    "test": "centos-logtest"
                }
            },
            "spec": {
                "replicas": "${{REPLICAS}}",
                "template": {
                    "metadata": {
                        "generateName": "centos-logtest-",
                        "labels": {
                            "run": "centos-logtest",
                            "test": "centos-logtest"
                        }
                    },
                    "spec": {
                        "containers": [
                            {
                                "env": [],
                                "image": "quay.io/openshifttest/ocp-logtest@sha256:16232868ba1143721b786dbabb3f7384645acb663fadb4af48e9ea1228a67635",
                                "imagePullPolicy": "Always",
                                "name": "centos-logtest",
                                "resources": {},
                                "volumeMounts": [
                                    {
                                        "name": "config",
                                        "mountPath": "/var/lib/svt"
                                    }
                                ],
                                "terminationMessagePath": "/dev/termination-log"
                            }
                        ],
                        "volumes": [
                            {
                                "name": "config",
                                "configMap": {
                                    "name": "logtest-config"
                                }
                            }
                        ]
                    }
                }
            }
        }
    ],
    "parameters": [
        {
            "name": "IDENTIFIER",
            "displayName": "identifier",
            "value": "1"
        },
        {
            "name": "REPLICAS",
            "displayName": "Replicas",
            "value": "1"
        }
    ]
}

